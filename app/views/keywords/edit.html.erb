<div class="row">
  <div class="col-lg-12">
    <br>
    <h1 class="form_group">編集ページ</h1>
    <br>
    <div class="contents row">
      <div class="container1">
        <%= form_with(model: @keyword, local: true) do |f| %>
          <%= render 'layouts/error_messages', model: f.object %>
          <%= f.label :保存ファイル名 %><br>
          <%= f.text_field :save_filename, placeholder: "利用ファイル名", class: "form-control1" %>
          <%= f.submit "登録"%>
        <% end %>
      </div>
    </div>
  </div>
</div>